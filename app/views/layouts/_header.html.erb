<div class="container-fluid">
  
  <h5 class="animated fadeIn">The doctor is now seeing <%= current_user %></h5>
      <div class="jumbotron">
        <h4 class="animated bounceInDown"><img src="/images/logo.png"width=7% height=7%>   personalized health care recommendations and reminders</h4>
        <h1 class="animated wobble">Doctor Zorders, MD    </h1>  
      </div>
    <audio>
      <source src="/images/elevate.mp3" type="audio/mpeg" autoplay>
      Your browser does not support the audio element.
  </audio>
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button"
                class="navbar-toggle collapsed"
                data-toggle="collapse"
                data-target="#navbar"
                aria-expanded="false"
                aria-controls="navbar">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      </div>
      <div id="navbar" class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
          <li <%= 'class="active"' if @active == 'home'  %>><a href="/">home</a></li>
          <!-- <li <%= 'class="active"' if @active == 'items' %>><a href="/items">Shop</a></li> -->
          <li <%= 'class="active"' if @active == 'about' %>><a href="/about">about</a></li>
        </ul>
        <ul class="nav navbar-nav navbar-right">
          <!-- <li><%#= link_to "Items", items_path %></li>
          <li><%#= link_to "New Item", new_item_path %></li> -->
          <% if signed_in? %>
            <li><%= link_to "search", user_path(current_user) %></li>
            <li><%= link_to "edit profile", edit_user_path(current_user) %></li>
            <li><%= link_to "sign out", signout_path, method: 'delete' %></li>
          <% else %>
            <li><%= link_to "sign up", signup_path %></li>
            <li><%= link_to "sign in", signin_path %></li>
          <% end %>
        </ul>
      </div><!--/.nav-collapse -->
    </div><!--/.container-fluid -->
  </nav>
</div>
</div>